    "gsap": "^3.13.0",
    "pinia": "^3.0.3",
    "vitest": "^3.2.3",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.9"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.11.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^24.0.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/language-core": "^2.2.10",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.2.0",
    "npm-run-all": "^4.1.5",
