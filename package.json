    "pinia": "^3.0.4",
    "vitest": "^4.0.18",
    "vue": "^3.5.29",
    "vue-router": "^5.0.3",
    "vuetify": "^4.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.16.1",
    "@tsconfig/node18": "^18.2.6",
    "@types/node": "^25.3.2",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.7.0",
    "@vue/language-core": "^3.2.5",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^10.0.2",
    "eslint-plugin-vue": "^10.8.0",
