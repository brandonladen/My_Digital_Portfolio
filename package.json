    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "gh-pages": "^6.3.0",
    "gsap": "^3.13.0",
    "pinia": "^3.0.3",
    "vitest": "^3.2.4",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuetify": "^3.9.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/language-core": "^3.0.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "npm-run-all": "^4.1.5",
