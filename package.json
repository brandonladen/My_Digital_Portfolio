    "axios": "^1.13.2",
    "bootstrap": "^5.3.8",
    "gh-pages": "^6.3.0",
    "gsap": "^3.14.2",
    "pinia": "^3.0.4",
    "vitest": "^4.0.15",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4",
    "vuetify": "^3.11.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.15.0",
    "@tsconfig/node18": "^18.2.6",
    "@types/node": "^25.0.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/language-core": "^3.1.8",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.96.0",
